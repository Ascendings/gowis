{% if Flash.InfoMsg %}
  <div class="row">
    <div class="alert alert-info">
      {{ Flash.InfoMsg }}
    </div>
  </div>
{% endif %}

{% if Flash.SuccessMsg %}
  <div class="row">
    <div class="alert alert-success">
  		{{ Flash.SuccessMsg }}
    </div>
  </div>
{% endif %}

{% if Flash.WarningMsg %}
  <div class="row">
    <div class="alert alert-warning">
  		{{ Flash.WarningMsg }}
    </div>
  </div>
{% endif %}

{% if Flash.ErrorMsg %}
  <div class="row">
    <div class="alert alert-danger">
      {{ Flash.ErrorMsg }}
    </div>
  </div>
{% endif %}
